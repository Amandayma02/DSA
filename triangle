public class Solution {
    public static void nTriangle(int n) {
        
         int i ,j;
        for (i = 1; i <= n; i++) {
            // inner loop to handle number of columns
            for (j = 1; j <= i; j++) {
                // printing column values upto the row
                // value.
                System.out.print(i + " ");
            }

            // print new line for each row
            System.out.println();
        }
    }
}
